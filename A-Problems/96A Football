#include <iostream>
#include <string>
 
using namespace std;
 
int check(string s,char c) {
    short int count=0;
    for (int i = 0; i < s.size(); ++i) {
        if (s[i]==c) {
            count++;
            if (count > 6) return true;
        }
        else count = 0;
    }
    return false;
}
 
