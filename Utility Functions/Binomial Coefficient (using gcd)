#include <iostream>

using namespace std;

int gcd(int a, int b) {
    return (b ? gcd(b, a % b) : a);
}

int ncr(int n, int r) {
    int p = 1, k = 1, m = 1;
    if (r > n - r)
        r = n - r;

    while (r) {
        p *= n--; k *= r--;
        m = gcd(p, k);
        p /= m; k /= m;
    }

    return p;
}

int main() {
    int n, r;
    cin >> n >> r;
    cout << ncr(n, r);
    return 0;
}
